function removeKeyFromStorage(e){try{localStorage.removeItem(e),localStorage.removeItem(e+"_expiresIn")}catch(t){return!1}return!0}function getKeyFromStorage(e){var t=Date.now(),r=localStorage.getItem(e+"_expiresIn");if((void 0===r||null===r)&&(r=0),t>r)return removeKeyFromStorage(e),1;try{var o=localStorage.getItem(e);return o}catch(a){return 1}}function setKeyStorage(e,t,r){r=void 0===r||null===r?1800:Math.abs(r);var o=Date.now(),a=o+1e3*r;try{localStorage.setItem(e,t),localStorage.setItem(e+"_expiresIn",a)}catch(n){return!1}return!0}