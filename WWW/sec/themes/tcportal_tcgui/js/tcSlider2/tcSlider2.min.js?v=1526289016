$.widget("TimoCom.tcSlider2",{timeout:null,actualSlide:0,sliding:!1,runAutomatically:!1,options:{speed:800,startSlide:1,run:!0,pause:4e3,displaySlideCount:1,easing:"linear",directLinkSpeed:500,stopOnMouseOver:!0,recognizeDOMChanges:!1},_create:function(){{var t=this;this.element}t.initOptions(),t.runAutomatically=t.options.run,$(window).resize(function(){t.runAutomatically&&t._update()}),t._handleInteraction(),t._update(),t.slideTo(t.options.startSlide,0)},initOptions:function(){var t=this,e=this.element,i="";for(var n in t.options)(e.attr("data-"+n)||e.attr("data-tcSlider-"+n)||""===e.attr("data-"+n)||""===e.attr("data-tcSlider-"+n))&&(i=e.attr("data-"+n)||e.attr("data-tcSlider-"+n),t.options[n]=$.isNumeric(i)?parseInt(i,10):-1!==i.indexOf("|")?i.split("|"):$.isArray(t.options[n])?[i]:null===t.options[n]?window[i]:t.options[n]===!0||t.options[n]===!1?"true"===i?!0:!1:i)},_handleInteraction:function(){var t=this,e=this.element;e.on("swipeleft",function(e){e.returnValue=!1,e.preventDefault&&e.preventDefault(),e.stop&&e.stop(),t.clearTimeout(),t.slideTo(parseInt(t.actualSlide,10)+1,t.options.directLinkSpeed)}),e.on("swiperight",function(e){e.returnValue=!1,e.preventDefault&&e.preventDefault(),e.stop&&e.stop(),t.clearTimeout(),t.slideTo(parseInt(t.actualSlide,10)-1,t.options.directLinkSpeed)}),e.on("swipeup",function(){$("html, body").animate({scrollTop:500},500)}),e.on("click",".carousel-indicators > li",function(e){e.returnValue=!1,e.preventDefault&&e.preventDefault(),e.stop&&e.stop(),t.clearTimeout(),t.slideTo(parseInt($(this).attr("data-tcSlider-page"),10),t.options.directLinkSpeed)}),e.on("click",".carousel-controlbar > .carousel-control",function(e){e.returnValue=!1,e.preventDefault&&e.preventDefault(),e.stop&&e.stop(),t.sliding||(t.clearTimeout(),"prev"===$(this).attr("data-tcSlider-direction")?t.slideTo(parseInt(t.actualSlide,10)-1,t.options.directLinkSpeed):t.slideTo(parseInt(t.actualSlide,10)+1,t.options.directLinkSpeed))}),e.on({mouseenter:function(){t.options.stopOnMouseOver&&t.pause()},mouseleave:function(){t.options.stopOnMouseOver&&t.runAutomatically&&t.run()}})},prependSlides:function(){var t=this.element,e=[null],n=$(".item:not(.clone)",t);for(i=0;1>i;i++)e[i]=$(n[n.length-1]).clone().addClass("clone");$(".tcSliderContainer",t).prepend(e)},appendSlides:function(){var t=this,e=this.element,n=[null],o=$(".item:not(.clone)",e);for(i=0;i<(t.options.displaySlideCount>1?t.options.displaySlideCount:1);i++)n[i]=$(o[i]).clone().addClass("clone");$(".tcSliderContainer",e).append(n)},getSlideWidth:function(){var t=this.element,e=$(".item",t);return $(e[0]).outerWidth(!0)},getMaxSlideHeight:function(){var t=this.element,e=$(".item",t),i=0;return e.each(function(){var t=$(this).height();t>i&&(i=t)}),i},getOffset:function(t){{var e=this,i=this.element;$(".item",i)}return e.getSlideWidth()*t*-1},_update:function(){var t=this,e=this.element,i=0,n=0,o=$(".item",e);o.remove(".clone"),$(".item:not(.clone)",e).length<=t.options.displaySlideCount?t.options.run=!1:(t.options.run=!0,t.appendSlides(),t.prependSlides(),o=$(".item",e),i=o.length,1===t.options.displaySlideCount?n=$(o[t.options.displaySlideCount+t.actualSlide]).height():(n=t.getMaxSlideHeight(),o.css({height:n})),0!==n&&e.css({height:n}).children(".tcSliderContainer").css({marginLeft:t.getOffset(t.actualSlide),width:t.getSlideWidth()*i}))},slideTo:function(t,e){var i=this,n=i.element,o=$(".item:not(.clone)",n).length;i.options.recognizeDOMChanges&&this._update(),$(".item:not(.clone)",n).length>i.options.displaySlideCount&&(i.sliding=!0,n.children(".tcSliderContainer").animate({marginLeft:i.getOffset(t)},{duration:e,easing:i.options.easing,complete:function(){i.sliding=!1,i.actualSlide=t,$("li[data-tcSlider-page]",$(".carousel-indicators",n)).removeClass("active"),$('li[data-tcSlider-page="'+i.actualSlide+'"]',$(".carousel-indicators",n)).addClass("active"),i.actualSlide===o+1?(i.clearTimeout(),i.slideTo(1,0)):0===i.actualSlide?(i.clearTimeout(),i.slideTo(o,0)):i.options.run&&i.slideToNext()}}))},slideToNext:function(){var t=this;t.timeout=window.setTimeout(function(){t.slideTo(t.actualSlide+1,t.options.speed)},t.options.pause)},run:function(){var t=this;t.options.run=!0,t.slideToNext()},pause:function(){var t=this;t.options.run=!1,t.clearTimeout()},clearTimeout:function(){var t=this;null!==t.timeout&&(window.clearTimeout(t.timeout),t.timeout=null)}});