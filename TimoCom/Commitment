        $(function() {
            $('#languageSelection').on('click', function() {
                var gaString = 'Oeffnen der Sprachauswahl in Summe';
                    ga('globalTracker.send','ClickToOpenChangeLanguage', 'CountrySelectBox', gaString );
            });
            $('#languageSelectionBox a[data-country]').on('click', function() {
                var gaString = 'VonSprache:en NachSprache:'+ $(this).attr("data-country");
                    ga('globalTracker.send', 'ChangeLanguageHP', 'SelectCountry', gaString);
            });
        });
    </script>
