        <script>
            $(function() {
                $('#unCheckPartner').on('click', function() {
                    if($(this).is(':checked')) {
                        $('.partner:checked').removeAttr('checked');
                        $(this).removeAttr('checked');
                    }
                }).parent().css({cursor: 'pointer'});
                
                
                checkPartner = function() {
                    var form = $('#trkContactForm');
                    if($('.partner:checked', form).length > 0) {
                        return true;
                    }
                    else {
                        return false;
                    }
                };
            });
        </script>
