<script>
    $(document).ready(function(){
        $('#video-container').append($($('.video-item').children()[0]).clone());
        $($('.load-video')[0]).addClass('video-active');
    });
    $('.load-video').click(function () {
        $('#video-container').children().remove();
        $(this).children('.video-item').children().clone().appendTo($('#video-container'));
        $('.load-video').removeClass('video-active');
        $(this).addClass('video-active');
    });
</script></div>
	
	

